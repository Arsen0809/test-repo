{
  "name": "test-repo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build-ts",
    "start": "npm run build && npm run watch",
    "dev": "ts-node-dev --respawn --transpileOnly ./",
    "build-ts": "tsc -p src",
    "watch-ts": "tsc -w -p src",
    "serve": "cross-env DEBUG=ts-express:* nodemon bin/www",
    "test": "cross-env NODE_ENV=test TS_NODE_PROJECT=test mocha --reporter spec --compilers ts:ts-node/register -r tsconfig-paths/register test/unit/**/*.test.ts",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Express\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve\"",
    "migrate": "knex migrate:latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pritisolanki/test-repo.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pritisolanki/test-repo/issues"
  },
  "homepage": "https://github.com/pritisolanki/test-repo#readme",
  "devDependencies": {
    "@types/body-parser": "^1.16.5",
    "@types/bookshelf": "^0.9.6",
    "@types/chai": "^4.0.4",
    "@types/dotenv": "^4.0.1",
    "@types/express": "^4.0.37",
    "@types/knex": "0.0.61",
    "@types/lusca": "^1.6.1",
    "@types/mocha": "^2.2.43",
    "@types/mock-knex": "^0.4.1",
    "@types/morgan": "^1.7.32",
    "@types/node": "^8.10.52",
    "@types/supertest": "2.0.3",
    "@types/validatorjs": "^3.7.0",
    "chai": "^4.1.2",
    "concurrently": "^3.5.0",
    "cross-env": "^5.0.5",
    "debug": "^3.0.1",
    "mocha": "^3.5.3",
    "mock-knex": "^0.3.9",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "tsconfig-paths": "^2.3.0",
    "tslint": "^5.7.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "3.5.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bookshelf": "^0.15.1",
    "chai-http": "^4.3.0",
    "cross-env": "^5.2.0",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "knex": "0.13.0",
    "lusca": "^1.6.1",
    "morgan": "^1.9.1",
    "npm": "^6.11.2",
    "pg": "^7.12.1",
    "validatorjs": "^3.15.1"
  }
}
